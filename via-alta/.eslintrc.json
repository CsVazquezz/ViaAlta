{
  "extends": ["next", "next/core-web-vitals", "airbnb"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "react/jsx-filename-extension": [
      1,
      { "extensions": [".tsx", ".jsx"] }
    ],
    "import/extensions": ["error", "ignorePackages", {
      "ts": "never",
      "tsx": "never"
    }],
    "import/prefer-default-export": "off",
     "no-console": "off"
  }
}
